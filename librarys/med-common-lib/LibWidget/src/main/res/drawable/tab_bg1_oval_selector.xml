            <solid android:color="@color/tab_bg1_oval_selected" />
            <corners android:radius="@dimen/Tab_bg1_radius" />
            <!--<padding
                android:top="8dp"
                android:bottom="8dp"
                android:left="30dp"
                android:right="30dp"
                />-->
