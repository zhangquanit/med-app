    <color name="ui_bg">#F6F7F8</color>
    <!--透明 颜色-->
    <color name="ui_transparent">#00000000</color>

    <!-- ~~~~~~~~~~~~按钮~~~~~~~~~~~~~ -->
    <!--按钮不可点击背景色-->
    <color name="button_enable_false">@color/ui_enable_false</color>
    <!-- ·······纯色按钮········ -->
    <!--蓝色按钮-->
    <color name="button_blue">@color/ui_blue</color>
    <color name="button_blue_pressed">#4BA1FF</color>
    <!--红色按钮-->
    <color name="button_red">@color/ui_red</color>
    <color name="button_red_pressed">#FF5959</color>
    <!--橙色按钮-->
    <color name="button_orange">@color/ui_orange</color>
    <color name="button_orange_pressed">#FFA833</color>
    <!--灰色按钮-->
    <color name="button_gray">#F1F1F1</color>
    <!-- ·······框按钮········ -->
    <color name="button_border_enable_false">@color/ui_white</color>
    <!--蓝框按钮-->
    <!--蓝框按下背景色-->
    <color name="button_border_blue_pressed">#eaf4ff</color>
    <!--橙框按钮-->
    <!--橙框按下背景色-->
    <color name="button_border_orange_pressed">#fff6ea</color>
    <!--灰框按钮-->
    <color name="button_border_gray">#CCCCCC</color>
    <!-- ·······button文字····框按钮文字按下···· -->
    <color name="button_text_black">@color/ui_black1</color>
    <color name="button_text_blue_pressed">#3596FF</color>
    <color name="button_text_orange_pressed">@color/button_orange</color>

    <!-- ~~~~~~~~~~~~标签 的颜色值~~~~~~~~~~~~~ -->
    <!--灰色标签-->
    <color name="label_gray">#F5F5F5</color>
    <color name="label_border_blue_solid">#eff6ff</color>
    <color name="label_border_blue_stroke">@color/ui_blue</color>

    <!-- ~~~~~~~~~~~~TAB~~~~~~~~~~~~~ -->
    <color name="tab_slide_line">@color/ui_blue</color>
    <!--TAB文字颜色-->
    <color name="tab_text_standard">@color/ui_black1</color>
    <color name="tab_text_standard_selected">@color/ui_blue</color>
    <color name="tab_text_bg1">@color/ui_black1</color>
    <color name="tab_text_bg1_selected">@color/ui_white</color>
    <color name="tab_text_bg2">@color/ui_black2</color>
    <color name="tab_text_bg2_selected">@color/ui_blue</color>
    <!--2级有背景的Tab的选中和未选中背景色-->
    <color name="tab_bg2_oval_selected">#E2EFFF</color>
    <color name="tab_bg2_oval_unselected">#F5F5F5</color>
    <!--1级有背景的Tab的选中和未选中背景色-->
    <color name="tab_bg1_oval_selected">@color/ui_blue</color>
    <color name="tab_bg1_oval_unselected">@android:color/transparent</color>
    <!--1级背景Tab的外部大框颜色-->
    <color name="tab_bg1_oval_out">#F1F1F1</color>

    <!-- ~~~~~~~~~~~~Navigation~~~~~~~~~~~~~ -->
    <!--标题颜色-->
    <color name="navigation_title_black">@color/ui_black1</color>
    <color name="navigation_title_right_black">@color/ui_black1</color>
    <color name="navigation_title_right_blue">@color/ui_blue</color>
    <color name="navigation_title_right_blue_press">@color/ui_blue</color>
    <color name="navigation_title_v_black">@color/ui_black2</color>

    <!-- ~~~~~~~~~~~~搜索框~~~~~~~~~~~~~ -->
    <color name="search_text_color">@color/ui_black1</color>
    <color name="search_text_color_hint">#9A9A9A</color>
    <color name="search_text_color_search">@color/ui_blue</color>
    <color name="search_text_color_cancel">@color/ui_black1</color>
    <color name="search_bg">#F1F1F1</color>

