<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--基础颜色-->
    <!--白色-->
    <color name="ui_white">#FFFFFF</color>
    <!--橙色-->
    <color name="ui_orange">#FF9300</color>
    <!--红色-->
    <color name="ui_red">#FF3030</color>
    <!--蓝色-->
    <color name="ui_blue">#1E8AFF</color>
    <!--1级黑-->
    <color name="ui_black1">#2A2A2A</color>
    <!--2级黑-->
    <color name="ui_black2">#4A4A4A</color>
    <!--3级黑-->
    <color name="ui_black3">#6A6A6A</color>
    <!--4级黑-->
    <color name="ui_black4">#9A9A9A</color>
    <!--enable false 颜色-->
    <color name="ui_enable_false">#DDDDDD</color>
